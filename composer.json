{
    "name": "spryker/store-reference",
    "type": "library",
    "description": "StoreReference module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/kernel": "^3.30.0",
        "spryker/message-broker-extension": "^1.0.0",
        "spryker/oauth-client-extension": "^1.0.0",
        "spryker/store": "^1.2.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/log": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/log": "If you want to use the StoreReferenceMessageValidatorPlugin."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Zed\\StoreReference\\Helper\\": "tests/SprykerTest/Zed/StoreReference/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
